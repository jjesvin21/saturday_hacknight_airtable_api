{
  "short_name": "Notes",
  "name": "Add notes to Airtable",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["storage", "tabs", "activeTab"],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_popup": "airtable/build/index.html",
    "default_icon": {
      "16": "/images/img.png",
      "32": "/images/img.png",
      "48": "/images/img.png",
      "128": "/images/img.png"
    }
  },
  "icons": {
    "16": "/images/img.png",
    "32": "/images/img.png",
    "48": "/images/img.png",
    "128": "/images/img.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' https://music.yandex.ru/;"
  }
}
